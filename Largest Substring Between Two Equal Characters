class Solution:
    def maxLengthBetweenEqualCharacters(self, s: str) -> int:
        leng=0
        leng1=0
        for i in range(len(s)):
            for j in range(i+1,len(s)):
                if s[j]==s[i]:
                    leng=j-i
                    if leng>=leng1:
                        leng1=leng
        if leng1==1:
            return 0
        elif leng1==0:
            return -1
        else:
            return leng1-1
